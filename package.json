{
  "name": "bears-and-bulls",
  "version": "1.0.0",
  "description": "React Mock Stock Market trading app",
  "main": "server.js",
  "proxy": "http://localhost:3001",
  "repository": "git@github.com:smalldoorman/bears-and-bulls.git",
  "author": "Nema Darban <nema.darban@gmail.com>",
  "license": "MIT",
  "scripts": {
    "server": "node server.js",
    "client": "node scripts/start-client.js",
    "start": "concurrently \"nodemon server.js\" \"npm run client\"",
    "build": "node scripts/build.js",
    "seed": "node scripts/seedDB.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:watch": "jest --watch",
    "installDeps": "yarn && cd client && yarn"
  },
  "dependencies": {
    "@types/auth0-js": "^8.11.2",
    "auth0-js": "^9.5.0",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "body-parser": "^1.18.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "concurrently": "^3.5.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "echarts": "^4.0.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "jwt-decode": "^2.2.0",
    "mdbreact": "^4.3.0",
    "moment": "^2.22.1",
    "mongoose": "^5.0.16",
    "morgan": "^1.9.0",
    "object-assign": "4.1.1",
    "path": "^0.12.7",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-app": "^1.1.2",
    "react-bootstrap": "^0.32.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-moment": "^0.7.0",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "resolve": "1.6.0",
    "socket.io-client": "^2.1.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "testRegex": ".*?(\\.spec).js",
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "client/**/*.js",
      "client/**/*.jsx",
      "server/**/*.js*"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      ".*": "<rootDir>/node_modules/babel-jest/build/index.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|txt)$": "<rootDir>/utilities/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testURL": "localhost:8080",
    "coverageReporters": [
      "json",
      "html",
      "text",
      "lcov",
      "clover"
    ],
    "redux-thunk": "^2.2.0",
    "socket.io-client": "^2.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.46",
    "axios-mock-adapter": "^1.15.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babelify": "^8.0.0",
    "core-js": "^2.5.6",
    "nodemon": "^1.17.4",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1",
    "redux-saga": "^0.16.0",
    "redux-test-utils": "^0.2.2",
    "watchman": "^1.0.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
